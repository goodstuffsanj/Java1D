<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.personal.sutdbookingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".BookProf"
            android:parentActivityName=".HomePage"></activity>
        <activity android:name=".BookFacilities"
            android:parentActivityName=".HomePage"></activity>
        <activity android:name=".LoginPage" />
        <activity android:name=".Prof"
            android:parentActivityName=".BookProf"></activity>
        <activity android:name=".Facility"
            android:parentActivityName=".BookFacilities"></activity>
    </application>

</manifest>